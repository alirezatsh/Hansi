{
  "name": "hansi",
  "description": "This is CLI tool for setup and structuring your projects",
  "version": "0.0.6",
  "author": "alireza tashakkori",
  "bin": {
    "hansi": "./bin/run.js"
  },
  "bugs": "https://github.com/alirezatsh/Hansi/issues",
  "dependencies": {
    "@oclif/plugin-plugins": "^5",
    "inquirer": "^12.9.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/core": "^4.5.4",
    "@oclif/plugin-help": "^6.2.33",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin",
    "dist",
    "src/scripts",
    "src/guides",
    "oclif.manifest.json"
  ],
  "homepage": "https://github.com/alirezatsh/Hansi#readme",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "hansi",
    "dirname": "hansi",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "https://github.com/alirezatsh/Hansi",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npx oclif manifest && npx oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "npx oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
